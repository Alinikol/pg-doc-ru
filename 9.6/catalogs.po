#: catalogs.xml:8381(structname) catalogs.xml:8975(structname)
#: catalogs.xml:9068(structname) catalogs.xml:9519(structname)
#: catalogs.xml:9700(structname) catalogs.xml:10062(structname)
msgid "pg_namespace"
msgstr "pg_namespace"

#: catalogs.xml:1568(literal) catalogs.xml:1935(literal)
#: catalogs.xml:2054(literal) catalogs.xml:2326(literal)
#: catalogs.xml:2656(literal) catalogs.xml:3200(literal)
#: catalogs.xml:4283(literal) catalogs.xml:4381(literal)
