msgid "user_defined_type_category"
msgstr "user_defined_type_category"

#: information_schema.xml:6086(entry)
msgid "Currently always <literal>STRUCTURED</literal>"
msgstr "На данный момент всегда <literal>STRUCTURED</literal>"

